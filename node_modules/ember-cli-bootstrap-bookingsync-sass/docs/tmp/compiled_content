  <span class="nt">&lt;label</span> <span class="na">for=</span><span class="s">"filledTextarea"</span><span class="nt">&gt;</span>Comment<span class="nt">&lt;/label&gt;</span>
  <span class="nt">&lt;textarea</span> <span class="na">class=</span><span class="s">"form-control"</span> <span class="na">id=</span><span class="s">"filledTextarea"</span> <span class="na">rows=</span><span class="s">"5"</span><span class="nt">&gt;</span>
