{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "spread": true,
      "modules": true
    }
  },
  "rules": {
    "no-tabs": [
      "error"
    ],
    "quotes": [
      "error",
      "single",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ],
    "no-console": [
      "warn"
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "indent": [
      "error",
      2,
      { "SwitchCase": 1 }
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 1
      }
    ],
    "prefer-const": [
      "error"
    ],
    "object-shorthand": [
      "warn",
      "always"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-debugger": [
      "error"
    ],
    "no-prototype-builtins": [
      "error"
    ],
    "no-template-curly-in-string": [
      "error"
    ],
    "no-eval": [
      "error"
    ],
    "no-caller": [
      "error"
    ],
    "no-alert": [
      "error"
    ],
    "eqeqeq": [
      "error",
      "always"
    ],
    "dot-notation": [
      "error",
      {
        "allowKeywords": true
      }
    ],
    "array-bracket-newline": [
      "error",
      {
        "multiline": true
      }
    ],
    "array-bracket-spacing": [
      "error",
      "never"
    ],
    "block-spacing": [
      "error",
      "always"
    ],
    "brace-style": [
      "error",
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "camelcase": [
      "error",
      {
        "properties": "never"
      }
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "comma-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      "error",
      "last"
    ],
    "computed-property-spacing": [
      "error",
      "never"
    ],
    "func-call-spacing": [
      "error",
      "never"
    ],
    "key-spacing": [
      "error",
      {
        "beforeColon": false,
        "afterColon": true,
        "mode": "strict"
      }
    ],
    "keyword-spacing": [
      "error",
      {
        "before": true,
        "after": true
      }
    ],
    "no-trailing-spaces": [
      "error"
    ]
  },
  "env": {
    "es6": true,
    "browser": true,
    "embertest": true,
    "node": true
  },
  "globals": {
    "moment": true,
    "humps": true,
    "filesize": true,
    "PDFJS": true,
    "Headway": true
  }
}
