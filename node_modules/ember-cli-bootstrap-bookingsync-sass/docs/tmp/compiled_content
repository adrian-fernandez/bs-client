  <span class="nt">&lt;label</span> <span class="na">for=</span><span class="s">"filledInput"</span><span class="nt">&gt;</span>Filled state<span class="nt">&lt;/label&gt;</span>
  <span class="nt">&lt;input</span> <span class="na">class=</span><span class="s">"form-control"</span> <span class="na">id=</span><span class="s">"filledInput"</span> <span class="na">type=</span><span class="s">"text"</span>
         <span class="na">value=</span><span class="s">"Demonstrative filled state"</span><span class="nt">&gt;</span>
