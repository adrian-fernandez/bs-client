I"}<div class="row">
  <div class="col-md-3 col-md-push-9">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">
          <a href="#forms">Forms</a>
        </h3>
      </div>
      <div class="list-group">
        <a class="list-group-item" href="#basic-example">Basic example</a>
        <a class="list-group-item" href="#inline-form">Inline form</a>
        <a class="list-group-item" href="#horizontal-form">Horizontal form</a>
        <a class="list-group-item" href="#supported-controls">Supported controls</a>
        <a class="list-group-item" href="#static-control">Static control</a>
