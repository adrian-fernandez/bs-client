{
  "name": "ember-cli-concat",
  "version": "1.2.0",
  "description": "An Ember addon that enables you to concatinate Ember CLI's app and vendor files into a single JS file and a single CSS file",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "node tests/runner",
    "autotest": "mocha --watch --reporter spec tests/**/*-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sir-dunxalot/ember-cli-concat.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "broccoli": "^0.16.9",
    "broccoli-asset-rev": "^2.4.2",
    "chai": "^2.1.0",
    "chai-fs": "git://github.com/sir-dunxalot/chai-fs.git#master",
    "ember-cli": "^2.4.0",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-htmlbars": "^1.0.3",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-jshint": "^1.0.0",
    "ember-cli-release": "^0.2.9",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.5.1",
    "ember-resolver": "^2.0.3",
    "glob": "^7.0.5",
    "loader.js": "^4.0.1",
    "lodash": "^3.8.0",
    "mocha": "^3.0.2",
    "mocha-jshint": "^2.2.3",
    "through": "^2.3.7",
    "tmp-sync": "^1.0.1"
  },
  "dependencies": {
    "broccoli-file-remover": "^0.3.1",
    "broccoli-merge-trees": "^1.1.4",
    "broccoli-concat": "^3.0.4",
    "ember-cli-babel": "^5.1.6"
  },
  "keywords": [
    "ember-addon",
    "ember",
    "ember-cli",
    "concatenate",
    "vendor",
    "performance",
    "scripts"
  ],
  "ember-addon": {
    "before": "broccoli-asset-rev",
    "configPath": "tests/dummy/config",
    "after": [
      "ember-component-css",
      "ember-sprite"
    ]
  }
}
