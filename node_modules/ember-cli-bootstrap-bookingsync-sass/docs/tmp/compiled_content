    <span class="nt">&lt;li&gt;</span>M 14<span class="nt">&lt;/li&gt;</span> <span class="nt">&lt;li&gt;</span>T 15<span class="nt">&lt;/li&gt;</span> <span class="nt">&lt;li&gt;</span>W 16<span class="nt">&lt;/li&gt;</span> <span class="nt">&lt;li&gt;</span>T 17<span class="nt">&lt;/li&gt;</span> <span class="nt">&lt;li&gt;</span>F 18<span class="nt">&lt;/li&gt;</span>
